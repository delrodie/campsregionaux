{% block stylesheets %}
{{ parent() }}
    <style>
        ul{
            list-style-type: none;
        }
        li::before{
            display: inline-block;
            position: relative;
            width: 1.3em;
            height: 1.3em;
            top: 0.2rem;
            margin-right: 0.4rem;
            content: "";
            background-image: url({{ absolute_url(asset('assets/map/puce.png')) }});
            background-size: contain;
            padding-top: 10px;
        }
    </style>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script>
        var region = function (id) {
            switch (id) {
                {% for liste in listes %}
                case '{{ liste.nom }}':
                    document.querySelector('.information').textContent = "{{ liste.message }}"
                    break;
                {% endfor %}
                default :
                    var text = "c'est le defaut";
            }
        }

        var information = function(id){
            if(id !== undefined){
                region(id)
            }
        }
    </script>
{% endblock %}