{% extends 'base.html.twig' %}

{% block title %}Config{% endblock %}

{% block body %}
    <h1>Config</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ config.id }}</td>
            </tr>
            <tr>
                <th>Apikey</th>
                <td>{{ config.apikey }}</td>
            </tr>
            <tr>
                <th>SiteId</th>
                <td>{{ config.siteId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('backend_config_index') }}">back to list</a>

    <a href="{{ path('backend_config_edit', {'id': config.id}) }}">edit</a>

    {{ include('backend_config/_delete_form.html.twig') }}
{% endblock %}
